{
    "manifest_version": 3,
    "name": "dripped",
    "version": "1.0",
    "description": "a web extension that helps consumers make sustainable shopping decisions by estimating the environmental impact of apparel items",
    "permissions": ["storage", "activeTab", "scripting"],
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icons/pic_logo.png"
    },
    "host_permissions": [
      "<all_urls>"
    ]
  }  