{
    "manifest_version": 3,
    "name": "HTML Scraper",
    "version": "1.0",
    "description": "Scrapes the HTML of the current webpage and prints it to console.",
    "permissions": ["storage", "activeTab", "scripting"],
    "background": {
      "service_worker": "background.js",
      "type": "module"
    },
    "action": {
      "default_icon": "icons/icon16.png"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }  